{
  "manifest_version": 3,
  "name": "Leedz Precrime",
  "version": "1.0",
  "description": "AI Networking Agent",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Open Precrime"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js":[
        "js/parser.js",
        "js/linkedin_parser.js",
        "js/content.js"
      ],
      "css": ["css/leedz_layout.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "css/*",
        "js/*",
        "sidebar_new.html",
        "icons/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}